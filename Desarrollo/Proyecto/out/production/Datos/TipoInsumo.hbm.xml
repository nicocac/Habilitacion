<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="TipoInsumoDao" table="tipo_insumo">
        <meta attribute="class-description">
            This class contains the tipo insumo detail.
        </meta>
        <id name="id" type="int" column="tin_id">
            <generator class="native"/>
        </id>
        <property name="nombre" column="tin_nombre" type="string"/>
        <property name="descripcion" column="tin_descripcion" type="string"/>
        <property name="fechaAlta" column="tin_fecha_alta" type="java.util.Date"/>
        <property name="usuarioAlta" column="tin_usuario_alta" type="string"/>
        <property name="fechaBaja" column="tin_fecha_baja" type="java.util.Date"/>
        <property name="usuarioBaja" column="tin_usuario_baja" type="string"/>
        <property name="fechaUltMod" column="tin_fecha_ult_mod" type="java.util.Date"/>
        <property name="usuarioUltMod" column="tin_usuario_utl_mod" type="string"/>

    </class>
</hibernate-mapping>